{
  "name": "upwork-job-scraper-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --outDir build-ts && node ./scripts/generate-manifest.js && node ./scripts/validate-extension.js",
    "preview": "vite preview",
    "package:open": "pwsh ./scripts/load-extension.ps1",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\""
  },
  "dependencies": {
    "@sentry/browser": "^7.60.0",
    "cheerio": "^1.0.1",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.4.24",
    "prettier": "^3.7.1",
    "tailwindcss": "^3.4.8",
    "typescript": "^5.5.2",
    "typescript-eslint": "^8.48.0",
    "vite": "^5.4.0"
  }
}
